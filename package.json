{
	"name": "vscode-magit",
	"displayName": "Magit for VSCode",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-magit.magit",
				"title": "Open Magit"
			}
		],
		"keybindings": [
			{
				"command": "vscode-magit.magit",
				"key": "ctrl+shift+m",
				"mac": "cmd+shift+m",
				"when": "resourceScheme != magit"
			}
		]
	},
	"scripts": {
		"build": "cd src && npm run build && cd .. && tsc -p ./",
		"watch": "tsc -watch -p ./",
		"react": "cd src && npm run start"
	},
	"devDependencies": {
		"@types/node": "16.x",
		"@types/vscode": "^1.74.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-plugin-sonarjs": "^0.17.0",
		"eslint-plugin-unicorn": "^45.0.2",
		"typescript": "^4.9.3"
	},
	"dependencies": {
		"ignore": "^5.2.4",
		"parse-diff": "^0.11.1"
	}
}
